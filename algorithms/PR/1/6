using System;
using System.Net;
using System.Runtime.Intrinsics.Arm;

public class Noname
{
    public static void Main()
    {
        Console.WriteLine("Тiшевський Михйало КIб-1-23-4.0д");
        Console.WriteLine("Enter minutes passed after 00:00:");
        double Minutes = Convert.ToDouble(Console.ReadLine());
        double Hours = (Math.Truncate(Minutes / 60))%24;
        Console.WriteLine("= {0}:{1}", Hours, Minutes%60);
    }
}
